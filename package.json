{
  "name": "blinkit-clone",
  "version": "1.0.0",
  "description": "Blinkit e-commerce clone with cart, payment, and order tracking",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.0",
    "socket.io": "^4.7.2",
    "razorpay": "^2.9.2",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["blinkit", "ecommerce", "cart", "payment", "delivery"],
  "author": "Developer",
  "license": "MIT"
}